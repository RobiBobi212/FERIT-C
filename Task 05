#include<stdio.h>
#include<stdlib.h>
int najveci (int **a, int n, int m);

int main(){
        // a je matrica, m i n su dimenzije matrice
        int **a, m, n, i, j;
        
        // zbog testinih slucajeva potrebno je prvo unijeti velicinu n, a tek zatim m
        do{
            scanf("%d",&n);//upis i provjera scanf-a za n
        }while(n<2||n>5);//uvjeti za n
        do{
            scanf("%d",&m);//upis i provjera scanf-a za m
        }while(m<2||m>8);//uvjeti za m
        
        a=(int**)malloc(n*sizeof(int*));
        for(i=0;i<n;i++){
            a[i]=(int*)malloc(m*sizeof(int));
        }
        for(i=0;i<n;i++){
            for(j=0;j<m;j++){
                scanf("%d",(*(a+i)+j));
            }
        }
        
        //Ispis rezultata
        printf("\nREZULTATI:\n");
        printf("%d", najveci (a,n,m));
        for(i=0;i<n;i++) free(a[i]);
        free(a);
        return 0;
}
int najveci (int **a, int n, int m){
    int max, i,j;
    max=**a;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if (*(*(a+i)+j)>max) max=*(*(a+i)+j);
        }
    }
    return max;
        //ova funkcija treba vratiti vrijednost najveceg elementa u predanoj matrici
       //ovdje ide vas kod
}
