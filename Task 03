#include <stdio.h>
struct info{
    char ime[20];
    char prezime[20];
    int mbroj;
    float prosjek;
    char rod[20];
};
int main(void){
    struct info ucenik[40];
    unsigned int i, n;
    float max=0;
    int nmax=0;
    do{scanf("%u",&n);}while(n>40||n==0);
    for(i=0; i<n; i++){
        scanf("%19s",&ucenik[i].ime);
        scanf("%19s",&ucenik[i].prezime);
        scanf("%d",&ucenik[i].mbroj);
        scanf("%f",&ucenik[i].prosjek);
        scanf("%19s",&ucenik[i].rod);
        if(ucenik[i].prosjek>max)
        {
            max=ucenik[i].prosjek;
            nmax=i;
        }
    }
    printf("REZULTATI:\n");
    printf("%s %s",ucenik[nmax].ime, ucenik[nmax].prezime);
    return 0;
}
